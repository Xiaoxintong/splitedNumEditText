<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SplitNumEditText">
        <attr name="item_hint" format="string" />
        <attr name="item_max_len" format="integer" />
        <attr name="item_type" format="enum">
            <enum name="common" value="0"/>
            <enum name="bank_card_no" value="1"/>
            <enum name="identify_card_no" value="2"/>
            <enum name="phone" value="3"/>
        </attr>
        <attr name="item_text_color" format="color" />
        <attr name="item_text_size" format="dimension" />
    </declare-styleable>

</resources>